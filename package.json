{
	"name": "portfolio-piotr-morawski",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
		"lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
		"lint-fix": "eslint --fix . --ext ts --ext tsx --ext js --ext jsx",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "jest",
		"clean:functions": "rm -rf functions-build && mkdir functions-build",
		"postinstall": "netlify-lambda install functions",
		"serve:function-send-email": "netlify-lambda serve functions/send-email",
		"build:function-send-email": "netlify-lambda build functions/send-email"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"dotenv": "^8.2.0",
		"gatsby": "^2.24.80",
		"gatsby-background-image": "^1.1.2",
		"gatsby-image": "^2.4.21",
		"gatsby-plugin-google-fonts": "^1.0.1",
		"gatsby-plugin-postcss": "^2.3.12",
		"gatsby-plugin-preact": "^4.0.13",
		"gatsby-plugin-react-helmet": "^3.3.11",
		"gatsby-plugin-react-svg": "^3.0.0",
		"gatsby-plugin-sass": "^2.3.13",
		"gatsby-plugin-sharp": "^2.6.42",
		"gatsby-plugin-typescript": "^2.4.20",
		"gatsby-source-cloudinary": "^0.1.14",
		"gatsby-source-filesystem": "^2.3.34",
		"gatsby-transformer-sharp": "^2.5.18",
		"netlify-lambda": "^1.6.3",
		"node-sass": "^4.14.1",
		"preact": "^10.5.3",
		"preact-render-to-string": "^5.1.10",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^5.2.1",
		"react-transition-group": "^4.4.1"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.12.1",
		"@types/grecaptcha": "^3.0.1",
		"@types/jest": "^25.2.3",
		"@types/react-addons-css-transition-group": "^15.0.5",
		"@types/react-helmet": "^5.0.16",
		"@types/react-transition-group": "^4.4.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"babel-jest": "^25.5.1",
		"babel-preset-gatsby": "^0.5.14",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-typescript": "^7.2.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-loader": "^3.0.4",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.2",
		"eslint-plugin-react-hooks": "^1.7.0",
		"gatsby-plugin-eslint": "^2.0.8",
		"gatsby-plugin-root-import": "^2.0.5",
		"gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.12",
		"husky": "^4.3.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^25.5.4",
		"prettier": "^1.19.1",
		"react-test-renderer": "^16.13.1",
		"typescript": "^3.9.7"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && git add ."
		}
	}
}
