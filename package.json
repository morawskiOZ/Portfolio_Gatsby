{
	"name": "portfolio-piotr-morawski",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"build": "npx --node-arg '-r esm' gatsby build",
		"develop": "npx --node-arg '-r esm' gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
		"lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
		"lint-fix": "eslint --fix . --ext ts --ext tsx --ext js --ext jsx",
		"start": "npm run develop",
		"serve": "npx --node-arg '-r esm' gatsby serve",
		"clean": "gatsby clean",
		"test": "jest",
		"clean:functions": "rm -rf functions-build && mkdir functions-build",
		"postinstall": "netlify-lambda install functions",
		"serve:function-send-email": "netlify-lambda serve functions/send-email",
		"build:function-send-email": "netlify-lambda build functions/send-email",
		"update-npm-libs": "npx npm-check -u",
		"preload-fonts": "gatsby-preload-fonts"
	},
	"dependencies": {
		"axios": "^0.20.0",
		"dotenv": "^8.2.0",
		"esm": "^3.2.25",
		"gatsby": "^2.24.80",
		"gatsby-image": "^2.4.21",
		"gatsby-plugin-postcss": "^3.0.3",
		"gatsby-plugin-preact": "^4.0.16",
		"gatsby-plugin-preload-fonts": "^1.2.28",
		"gatsby-plugin-react-helmet": "^3.3.14",
		"gatsby-plugin-react-svg": "^3.0.0",
		"gatsby-plugin-sass": "^2.3.17",
		"gatsby-plugin-sharp": "^2.6.42",
		"gatsby-plugin-typescript": "^2.4.22",
		"gatsby-source-cloudinary": "^0.1.14",
		"gatsby-source-filesystem": "^2.3.35",
		"gatsby-transformer-sharp": "^2.5.18",
		"netlify-lambda": "^2.0.1",
		"node-sass": "^4.14.1",
		"preact": "^10.5.5",
		"preact-render-to-string": "^5.1.10",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-helmet": "^6.1.0",
		"react-transition-group": "^4.4.1"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.12.1",
		"@types/grecaptcha": "^3.0.1",
		"@types/jest": "^26.0.14",
		"@types/react-addons-css-transition-group": "^15.0.5",
		"@types/react-helmet": "^6.1.0",
		"@types/react-transition-group": "^4.4.0",
		"@typescript-eslint/eslint-plugin": "^4.4.1",
		"@typescript-eslint/parser": "^4.4.1",
		"babel-jest": "^26.5.2",
		"babel-preset-gatsby": "^0.5.14",
		"eslint": "^7.11.0",
		"eslint-config-airbnb-typescript": "^12.0.0",
		"eslint-config-prettier": "^6.13.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.4",
		"eslint-plugin-react-hooks": "^4.1.2",
		"gatsby-plugin-eslint": "^2.0.8",
		"gatsby-plugin-root-import": "^2.0.5",
		"gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.13",
		"husky": "^4.3.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.5.3",
		"prettier": "^2.1.2",
		"react-test-renderer": "^16.14.0",
		"typescript": "^4.0.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && git add ."
		}
	}
}
